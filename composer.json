{
    "name": "psprokofiev/rabbitmq-management-api",
    "type": "library",
    "description": "An object oriented wrapper for the RabbitMQ Management HTTP API",
    "homepage": "https://github.com/psprokofiev/php-rabbitmq-management-api",
    "keywords": [
        "rest",
        "message queue",
        "rabbitmq"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Fullmer",
            "email": "richardfullmer@gmail.com",
            "homepage": "https://github.com/richardfullmer"
        },
        {
            "name": "Pavel Prokofiev",
            "email": "ps.prokofiev@gmail.com",
            "homepage": "https://github.com/psprokofiev"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.3",
        "illuminate/http": "^8.60",
        "illuminate/support": "^8.60",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "RabbitMq\\ManagementApi\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "test/"
        }
    }
}
